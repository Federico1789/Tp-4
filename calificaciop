#include <stdio.h>

int main(void) {
	
	int nota;
	
	printf("Ingrese la calificación: ");
	scanf("%d", &nota);
	

	
	if (nota >= 90) {
		printf("Su Calificación: A.\n");
	} else if (nota >= 80 && nota <90) {
		printf("Su Calificación: B.\n");
	} else if (nota >= 70 && nota<80) {
		printf("Su Calificación: C.\n");
	} else if (nota >= 60 && nota <70) {
		printf("Su Calificación: D.\n");
	} else {
		printf("Su Calificación: F.\n");
	}
	
	return 0;
	}

